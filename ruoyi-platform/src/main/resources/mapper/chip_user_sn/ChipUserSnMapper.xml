<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.platform.chip.chip_user_sn.mapper.ChipUserSnMapper">
    
    <resultMap type="ChipUserSn" id="ChipUserSnResult">
        <result property="id" column="id"/>
        <result property="userId" column="user_id"/>
        <result property="phone" column="phone"/>
        <result property="nickname" column="nickname"/>
        <result property="sn" column="sn"/>
        <result property="thumbnail" column="thumbnail"/>
        <result property="status" column="status"/>
        <result property="expiredTime" column="expired_time"/>
        <result property="isDelete" column="is_delete"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>
    
    <sql id="selectChipUserSnVo">
        select id,
               user_id,
               phone,
               nickname,
               sn,
               thumbnail,
               status,
               expired_time,
               is_delete,
               create_time,
               update_time
        from chip_user_sn
    </sql>
    
    <select id="selectChipUserSnList"
            parameterType="ChipUserSn"
            resultMap="ChipUserSnResult">
        <include refid="selectChipUserSnVo"/>
        <where>
            <if test="userId != null ">and user_id like '%${userId}%'</if>
            <if test="phone != null  and phone != ''">and phone like '%${phone}%'</if>
            <if test="nickname != null  and nickname != ''">and nickname like concat('%', #{nickname}, '%')</if>
            <if test="sn != null  and sn != ''">and sn like '%${sn}%'</if>
            <if test="thumbnail != null  and thumbnail != ''">and thumbnail like '%${thumbnail}%'</if>
            <if test="status != null ">and status like '%${status}%'</if>
            <if test="expiredTime != null ">and expired_time like '%${expiredTime}%'</if>
            <if test="isDelete != null ">and is_delete like '%${isDelete}%'</if>
        </where>
    </select>
    
    <select id="selectChipUserSnById"
            parameterType="Long"
            resultMap="ChipUserSnResult">
        <include refid="selectChipUserSnVo"/>
        where id = #{id}
    </select>
    
    <insert id="insertChipUserSn" parameterType="ChipUserSn" useGeneratedKeys="true" keyProperty="id">
        insert into chip_user_sn
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="userId != null">user_id,</if>
            <if test="phone != null">phone,</if>
            <if test="nickname != null">nickname,</if>
            <if test="sn != null">sn,</if>
            <if test="thumbnail != null">thumbnail,</if>
            <if test="status != null">status,</if>
            <if test="expiredTime != null">expired_time,</if>
            <if test="isDelete != null">is_delete,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateTime != null">update_time,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="userId != null">#{userId},</if>
            <if test="phone != null">#{phone},</if>
            <if test="nickname != null">#{nickname},</if>
            <if test="sn != null">#{sn},</if>
            <if test="thumbnail != null">#{thumbnail},</if>
            <if test="status != null">#{status},</if>
            <if test="expiredTime != null">#{expiredTime},</if>
            <if test="isDelete != null">#{isDelete},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateTime != null">#{updateTime},</if>
        </trim>
    </insert>
    
    <update id="updateChipUserSn" parameterType="ChipUserSn">
        update chip_user_sn
        <trim prefix="SET" suffixOverrides=",">
            <if test="userId != null">user_id = #{userId},</if>
            <if test="phone != null">phone = #{phone},</if>
            <if test="nickname != null">nickname = #{nickname},</if>
            <if test="sn != null">sn = #{sn},</if>
            <if test="thumbnail != null">thumbnail = #{thumbnail},</if>
            <if test="status != null">status = #{status},</if>
            <if test="expiredTime != null">expired_time = #{expiredTime},</if>
            <if test="isDelete != null">is_delete = #{isDelete},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
        </trim>
        where id = #{id}
    </update>
    
    <delete id="deleteChipUserSnById" parameterType="Long">
        delete
        from chip_user_sn
        where id = #{id}
    </delete>
    
    <delete id="deleteChipUserSnByIds" parameterType="String">
        delete from chip_user_sn where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>