<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.platform.app.mydoc.doc_project.mapper.AppDocProjectMapper">
    
    <resultMap type="AppDocProject" id="AppDocProjectResult">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="intro" column="intro"/>
        <result property="createTime" column="create_time"/>
        <result property="modifyTime" column="modify_time"/>
        <result property="createUserId" column="create_user_id"/>
        <result property="role" column="role"/>
        <result property="roleValue" column="role_value"/>
        <result property="icon" column="icon"/>
        <result property="isWatermark" column="is_watermark"/>
        <result property="watermarkType" column="watermark_type"/>
        <result property="watermarkValue" column="watermark_value"/>
        <result property="isTop" column="is_top"/>
        <result property="visitor" column="visitor"/>
        <result property="isDelete" column="is_delete"/>
        <result property="sortField" column="sort_field"/>
    </resultMap>
    
    <sql id="selectAppDocProjectVo">
        select id,
               name,
               intro,
               create_time,
               modify_time,
               create_user_id,
               role,
               role_value,
               icon,
               is_watermark,
               watermark_type,
               watermark_value,
               is_top,
               visitor,
               is_delete,
               sort_field
        from app_doc_project
    </sql>
    
    <select id="selectAppDocProjectList"
            parameterType="AppDocProject"
            resultMap="AppDocProjectResult">
        <include refid="selectAppDocProjectVo"/>
        <where>
            <if test="name != null  and name != ''">and name like concat('%', #{name}, '%')</if>
            <if test="intro != null  and intro != ''">and intro like '%${intro}%'</if>
            <if test="modifyTime != null ">and modify_time like '%${modifyTime}%'</if>
            <if test="createUserId != null ">and create_user_id like '%${createUserId}%'</if>
            <if test="role != null ">and role like '%${role}%'</if>
            <if test="roleValue != null  and roleValue != ''">and role_value like '%${roleValue}%'</if>
            <if test="icon != null  and icon != ''">and icon like '%${icon}%'</if>
            <if test="isWatermark != null ">and is_watermark like '%${isWatermark}%'</if>
            <if test="watermarkType != null ">and watermark_type like '%${watermarkType}%'</if>
            <if test="watermarkValue != null  and watermarkValue != ''">and watermark_value like '%${watermarkValue}%'
            </if>
            <if test="isTop != null ">and is_top like '%${isTop}%'</if>
            <if test="visitor != null ">and visitor like '%${visitor}%'</if>
            <if test="isDelete != null ">and is_delete like '%${isDelete}%'</if>
            <if test="sortField != null  and sortField != ''">and sort_field like '%${sortField}%'</if>
        </where>
    </select>
    
    <select id="selectAppDocProjectById"
            parameterType="Long"
            resultMap="AppDocProjectResult">
        <include refid="selectAppDocProjectVo"/>
        where id = #{id}
    </select>
    
    <insert id="insertAppDocProject" parameterType="AppDocProject" useGeneratedKeys="true" keyProperty="id">
        insert into app_doc_project
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">name,</if>
            <if test="intro != null and intro != ''">intro,</if>
            <if test="createTime != null">create_time,</if>
            <if test="modifyTime != null">modify_time,</if>
            <if test="createUserId != null">create_user_id,</if>
            <if test="role != null">role,</if>
            <if test="roleValue != null">role_value,</if>
            <if test="icon != null">icon,</if>
            <if test="isWatermark != null">is_watermark,</if>
            <if test="watermarkType != null">watermark_type,</if>
            <if test="watermarkValue != null">watermark_value,</if>
            <if test="isTop != null">is_top,</if>
            <if test="visitor != null">visitor,</if>
            <if test="isDelete != null">is_delete,</if>
            <if test="sortField != null">sort_field,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="name != null and name != ''">#{name},</if>
            <if test="intro != null and intro != ''">#{intro},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="modifyTime != null">#{modifyTime},</if>
            <if test="createUserId != null">#{createUserId},</if>
            <if test="role != null">#{role},</if>
            <if test="roleValue != null">#{roleValue},</if>
            <if test="icon != null">#{icon},</if>
            <if test="isWatermark != null">#{isWatermark},</if>
            <if test="watermarkType != null">#{watermarkType},</if>
            <if test="watermarkValue != null">#{watermarkValue},</if>
            <if test="isTop != null">#{isTop},</if>
            <if test="visitor != null">#{visitor},</if>
            <if test="isDelete != null">#{isDelete},</if>
            <if test="sortField != null">#{sortField},</if>
        </trim>
    </insert>
    
    <update id="updateAppDocProject" parameterType="AppDocProject">
        update app_doc_project
        <trim prefix="SET" suffixOverrides=",">
            <if test="name != null and name != ''">name = #{name},</if>
            <if test="intro != null and intro != ''">intro = #{intro},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="modifyTime != null">modify_time = #{modifyTime},</if>
            <if test="createUserId != null">create_user_id = #{createUserId},</if>
            <if test="role != null">role = #{role},</if>
            <if test="roleValue != null">role_value = #{roleValue},</if>
            <if test="icon != null">icon = #{icon},</if>
            <if test="isWatermark != null">is_watermark = #{isWatermark},</if>
            <if test="watermarkType != null">watermark_type = #{watermarkType},</if>
            <if test="watermarkValue != null">watermark_value = #{watermarkValue},</if>
            <if test="isTop != null">is_top = #{isTop},</if>
            <if test="visitor != null">visitor = #{visitor},</if>
            <if test="isDelete != null">is_delete = #{isDelete},</if>
            <if test="sortField != null">sort_field = #{sortField},</if>
        </trim>
        where id = #{id}
    </update>
    
    <delete id="deleteAppDocProjectById" parameterType="Long">
        delete
        from app_doc_project
        where id = #{id}
    </delete>
    
    <delete id="deleteAppDocProjectByIds" parameterType="String">
        delete from app_doc_project where id in
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>